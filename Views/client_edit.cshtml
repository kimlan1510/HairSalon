<h1>Edit Client</h1>
<hr />
<h2>Edit Client: @Model.GetName()</h2>
<form action="/clients/edit/@Model.GetId()" method="post">
  <input type="hidden" name="_method" value="PATCH">
  <input name="client_id" type="hidden" value="@Model.GetId()">
  <label for="restaurant-name">Client Name</label>
  <input id="name" name="name" type="text">
  <button type="submit">Submit</button>
</form>
<h4><a href="/">Return home</a></h4>
